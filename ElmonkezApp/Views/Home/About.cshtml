@using ElmonkezApp.Models
@model dynamic
@{
    ViewData["Title"] = "About";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- inner page banner -->
<div id="inner_banner" class="section inner_banner_section_about">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="title-holder">
                        <div class="title-holder-cell text-left">
                            <h1 class="page-title">About Us</h1>
                            <ol class="breadcrumb">
                                <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                                <li class="active">About Us</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end inner page banner -->
<!-- section about Us -->
<div class="section padding_layout_1">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="main_heading text_align_center">
                        <h2>WE ARE A LEADING COMPANY</h2>
                        <p class="large">Fastest repair service with the best price!</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row about_blog">
            <div class="col-lg-6 col-md-6 col-sm-12 about_cont_blog">
                <div class="full text_align_left">
                    <h3>What we do</h3>
                    <p>
                        Providing experienced technical labor and providing them with good equipment that enables them to provide high-quality services capable of satisfying customers and thus ensuring them as permanent clients of the project....
                    </p>
                    <ul>
                        <li><i class="fa fa-check-circle"></i>Saving time and effort.</li>
                        <li><i class="fa fa-check-circle"></i>Good handling of problems facing customers.</li>
                        <li><i class="fa fa-check-circle"></i>Reply to inquiries and messages.</li>
                        <li><i class="fa fa-check-circle"></i>Establishing a good relationship between the employee and the customer.</li>
                    </ul>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-12 about_feature_img padding_right_0">
                <div class="full text_align_center"> <img class="img-responsive" src="~/client/images/it_service/about-img.jpg" alt="#" /> </div>
            </div>
        </div>
        <div class="row" style="margin-top: 35px">


        </div>
    </div>
</div>
<!-- end section about Us -->
<!-- section Service Process -->
<div class="section padding_layout_1 light_silver gross_layout">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="main_heading text_align_left">
                        <h2>Service Process</h2>
                        <p class="large">An easy and effective way to repair your home.</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="row">
                    <div class="col-md-4">
                        <div class="full">
                            <div class="service_blog_inner">
                                <div class="icon text_align_left"><img src="~/client/images/it_service/si1.png" alt="#" /></div>
                                <h4 class="service-heading">Fast service</h4>
                                <p>Responding to inquiries and messages and being able to solve problems.</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="full">
                            <div class="service_blog_inner">
                                <div class="icon text_align_left"><img src="~/client/images/it_service/si2.png" alt="#" /></div>
                                <h4 class="service-heading">Secure payments</h4>
                                <p>Providing a safe, good and efficient electronic payment service that can be easily dealt with.</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="full">
                            <div class="service_blog_inner">
                                <div class="icon text_align_left"><img src="~/client/images/it_service/si3.png" alt="#" /></div>
                                <h4 class="service-heading">Expert team</h4>
                                <p>It contains an organized work team and has a quick response and problem solving response.</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="full">
                            <div class="service_blog_inner">
                                <div class="icon text_align_left"><img src="~/client/images/it_service/si4.png" alt="#" /></div>
                                <h4 class="service-heading">Affordable services</h4>
                                <p>We are distinguished by fantastic prices with our service offers.</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="full">
                            <div class="service_blog_inner">
                                <div class="icon text_align_left"><img src="~/client/images/it_service/si5.png" alt="#" /></div>
                                <h4 class="service-heading">Quality assurance</h4>
                                <p>We provide customers with confidence with a quality service guarantee.</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="full">
                            <div class="service_blog_inner">
                                <div class="icon text_align_left"><img src="~/client/images/it_service/si6.png" alt="#" /></div>
                                <h4 class="service-heading">Award winning</h4>
                                <p>Number one in our field with the testimony of our clients in home maintenance services.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end section Service Process -->
<!--  section Employee-->
<div class="section padding_layout_1">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="main_heading text_align_left">
                        <h2>Experienced Staff</h2>
                        <p class="large">Our experts have been featured in press numerous times.</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            @foreach (Employee Emp in Model.EmployeeList)
            {
                <div class="col-md-3 col-sm-6">
                    <div class="full team_blog_colum">
                        <div class="it_team_img"> <img class="img-responsive" src="/@Emp.Image" alt="#"> </div>
                        <div class="team_feature_head">
                            <h4>@Emp.Name</h4>
                        </div>

                    </div>
                </div>
            }


        </div>
    </div>
</div>
<!-- End section Employee -->
<!-- section Review -->
